<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- Hibernate Generator by Javier Hernandez -->
<hibernate-mapping>
    <class name="es.sm2.openppm.core.model.impl.Configuration" table="configuration">
        <id name="idConfiguration" type="java.lang.Integer">
            <column name="idConfiguration" />
            <generator class="native">
                <param name="sequence">configuration_SEQ</param>
            </generator>
        </id>
        <many-to-one name="contact" class="es.sm2.openppm.core.model.impl.Contact" fetch="select">
            <column name="idContact" not-null="true" />
        </many-to-one>
        <property name="name" type="string">
            <column name="name" length="100" not-null="true" />
        </property>
        <property name="value" type="string">
            <column name="value" length="1000" />
        </property>
        <property name="type" type="string">
            <column name="type" length="100" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
