            <column name="idTimelineType" />
        </many-to-one>
        <many-to-one name="documentproject" class="es.sm2.openppm.core.model.impl.Documentproject" fetch="select">
            <column name="idDocumentProject" />
        </many-to-one>
        <property name="timelineDate" type="date">
            <column name="timelineDate" length="10" not-null="true" />
        </property>
        <property name="name" type="string">
            <column name="name" length="50" not-null="true" />
        </property>
        <property name="description" type="string">
            <column name="description" length="200" />
        </property>
        <property name="importance" type="string">
            <column name="importance" length="50" />
        </property>
    </class>
</hibernate-mapping>
