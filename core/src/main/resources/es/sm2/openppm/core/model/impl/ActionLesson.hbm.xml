<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="es.sm2.openppm.core.model.impl.ActionLesson" table="actionlesson">
        <id name="idActionLesson" type="java.lang.Integer">
            <column name="idActionLesson" />
            <generator class="native">
                <param name="sequence">actionLesson_SEQ</param>
            </generator>
        </id>
        <property name="name" type="string">
            <column name="name" length="300" />
        </property>
        <property name="importance" type="string">
            <column name="importance" length="100" />
        </property>
        <many-to-one name="learnedLesson" class="es.sm2.openppm.core.model.impl.LearnedLesson" fetch="select">
            <column name="idLearnedLesson" not-null="true" />
        </many-to-one>
    </class>
</hibernate-mapping>