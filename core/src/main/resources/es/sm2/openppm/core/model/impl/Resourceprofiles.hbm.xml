<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- Hibernate Generator by Javier Hernandez -->
<hibernate-mapping>
    <class name="es.sm2.openppm.core.model.impl.Resourceprofiles" table="resourceprofiles">
        <id name="idProfile" type="int">
            <column name="IdProfile" />
            <generator class="native">
                <param name="sequence">resourceprofiles_SEQ</param>
            </generator>
        </id>
        <property name="profileName" type="string">
            <column name="ProfileName" length="50" />
        </property>
        <property name="description" type="string">
            <column name="Description" length="100" />
        </property>
        <set name="employees" inverse="true" lazy="true" table="employee" fetch="select">
            <key>
                <column name="idProfile" />
            </key>
            <one-to-many class="es.sm2.openppm.core.model.impl.Employee" />
        </set>
    </class>
</hibernate-mapping>
