<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="es.sm2.openppm.core.model.impl.Notificationproject" table="notificationproject">
        <id name="idNotificationProject" type="java.lang.Integer">
            <column name="idNotificationProject" />
            <generator class="native">
                <param name="sequence">notificationproject_SEQ</param>
            </generator>
        </id>
        <many-to-one name="notificationType" class="es.sm2.openppm.core.model.impl.Notificationtype" fetch="select">
            <column name="idNotificationType" not-null="true" />
        </many-to-one>
        <many-to-one name="project" class="es.sm2.openppm.core.model.impl.Project" fetch="select">
            <column name="idProject" not-null="true" />
        </many-to-one>
        <property name="status" type="string">
            <column name="status" length="8" not-null="true" />
        </property>
        <property name="mode" type="string">
            <column name="notificationMode" length="11" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
